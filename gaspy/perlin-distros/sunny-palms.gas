// seed: factor (seeds/mÂ²), perlin offset, perlin spread
// templates: space-separated
// size: from, to, additional perlin factor

[perlin_plant_profile]
{
    // defaults
    size = 0.8 1.2;

    // flowers: red
    [*] { seed = 0.1, -0.1, 8; templates = flowers_grs_red; size = 1.6 2.4; }
    // groundcover
    [*] { seed = 0.1, 0, 4; templates = foliage_grs_01; }
    // trees: palms
    [*] { seed = 0.05, -0.5, 16; templates = tree_glb_palm_sway_02; }
}
